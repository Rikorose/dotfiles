Host github.com
  HostName github.com
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa

Host gitlab.cs.fau.de
  HostName gitlab.cs.fau.de
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes

